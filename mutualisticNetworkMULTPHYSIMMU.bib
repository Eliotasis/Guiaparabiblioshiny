@ARTICLE{Simmons2019154,
author={Simmons, B.I. and Cirtwill, A.R. and Baker, N.J. and Wauchope, H.S. and Dicks, L.V. and Stouffer, D.B. and Sutherland, W.J.},
title={Motifs in bipartite ecological networks: uncovering indirect interactions},
