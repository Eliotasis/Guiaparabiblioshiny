@ARTICLE{Olesen20101,
author={Olesen, J.M. and Dupont, Y.L. and O'Gorman, E. and Ings, T.C. and Layer, K. and Melián, C.J. and Trøjelsgaard, K. and Pichler, D.E. and Rasmussen, C. and Woodward, G.},
title={From Broadstone to Zackenberg. Space, Time and Hierarchies in Ecological Networks},
