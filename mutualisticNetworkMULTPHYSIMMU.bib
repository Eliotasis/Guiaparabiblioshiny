@ARTICLE{Minoarivelo20141250,
author={Minoarivelo, H.O. and Hui, C. and Terblanche, J.S. and Kosakovsky Pond, S.L. and Scheffler, K.},
title={Detecting phylogenetic signal in mutualistic interaction networks using a Markov process model},
journal={Oikos},
