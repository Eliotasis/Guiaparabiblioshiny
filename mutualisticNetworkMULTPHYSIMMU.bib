@ARTICLE{Encinas-Viso2016457,
author={Encinas-Viso, F. and Alonso, D. and Klironomos, J.N. and Etienne, R.S. and Chang, E.R.},
title={Plant-mycorrhizal fungus co-occurrence network lacks substantial structure},
