@ARTICLE{Calatayud201753,
author={Calatayud, J. and Madrigal-González, J. and Gianoli, E. and Hortal, J. and Herrero, A.},
title={Uneven abundances determine nestedness in climbing plant-host interaction networks},
journal={Perspectives in Plant Ecology, Evolution and Systematics},
year={2017},
volume={26},
pages={53-59},
doi={10.1016/j.ppees.2017.04.003},
note={cited By 7},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85018502928&doi=10.1016%2fj.ppees.2017.04.003&partnerID=40&md5=b62226362f0dfc255f4bc35da0cc4bad},
affiliation={Grupo de Ecología Forestal y Restauración, Departamento de Ciencias de la Vida, Universidad de Alcalá, Edificio de Ciencias, Ctra. Madrid-Barcelona km. 33,6, Alcalá de Henares, Madrid, 28871, Spain; Department of Biogeography and Global Change, Museo Nacional de Ciencias Naturales (MNCN-CSIC), C/José Gutiérrez Abascal 2, Madrid, 28006, Spain; Departamento de Biología, Universidad de La Serena, Casilla 554, La Serena, Chile; Departamento de Botánica, Universidad de Concepción, Casilla 160-C, Concepción, Chile; School of Forestry, Northern Arizona University, Flagstaff, AZ86011, United States; Departamento de Biología Vegetal y Ecología, Universidad del País Vasco, Facultad de Ciencia y Tecnología, B° Sarriena s/n, Leioa (Bizkaia), Basque Country  48940, Spain},
abstract={Nestedness is a common pattern in interaction networks. However, its ecological and evolutionary meaning is under debate. Evidence shows that nestedness in mutualistic networks may be just a consequence of the species–abundance distribution. This has been questioned as abundance itself could be influenced by differences in generalism between species. Host-parasite networks in plant communities also show nested patterns, but their relationship with abundance has been seldom addressed. Importantly, an assessment of the potentially different effect of the number of interacting species (i.e. generalism levels) on the size of parasite and host populations can help understanding the role of abundance in determining both generalism and nestedness. Here we show that nestedness follows abundance expectations in an interaction network of climbing plants (i.e. structural parasites) and their tree and shrub hosts. Our results also point to a direct effect of abundance on both nestedness and generalism levels because species degree does not deviate from abundance expectations for both climbing plants and their hosts. Further, we found a similar level of discordance between generalization (a generalism measure independent of species abundance) and abundance for both parties. Our findings provide evidence that the factors underlying uneven abundance distributions can induce nestedness in interaction networks. We stress the importance of neutral processes related to species dominance as major determinants of nestedness in host-parasite networks. © 2017 Elsevier GmbH},
author_keywords={Climbing-plant communities;  Dominance;  Host-parasite networks;  Nestedness;  Neutral interaction processes;  Species abundance},
