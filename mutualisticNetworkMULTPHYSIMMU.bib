@ARTICLE{Dáttilo2013374,
author={Dáttilo, W. and Rico-Gray, V. and Rodrigues, D.J. and Izzo, T.J.},
title={Soil and vegetation features determine the nested pattern of ant-plant networks in a tropical rainforest},
