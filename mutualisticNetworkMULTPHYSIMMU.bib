@ARTICLE{Sfair2018236,
author={Sfair, J.C. and Weiser, V.L. and Martins, F.R. and Vidal, M.M. and Guimarães, P.R., Jr.},
title={Species traits and abundance influence the organization of liana–tree antagonistic interaction},
